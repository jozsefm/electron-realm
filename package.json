{
  "name": "electron-realm",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder build -mwl",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "rebuild": "electron-builder install-app-deps"
  },
  "dependencies": {
    "realm": "^2.3.3",
    "source-map-support": "^0.5.4"
  },
  "devDependencies": {
    "electron": "1.8.4",
    "electron-builder": "^20.8.1",
    "electron-webpack": "^2.0.1",
    "webpack": "^4.4.1"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "build": {
    "appId": "com.paperpile.plugin",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "electronWebpack": {
    "title": "Electron Realm POC"
  }
}
